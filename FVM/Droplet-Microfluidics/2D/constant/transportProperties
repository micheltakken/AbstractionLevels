FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water oil);

water
{
    transportModel  Newtonian;
    nu              1e-06;
    rho             1000;
}

oil
{
    transportModel  Newtonian;
    nu              1e-06;
    rho             1000;
}
sigma            0.005;

