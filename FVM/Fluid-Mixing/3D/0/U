FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    right
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    Inlet1
    {
        type            fixedValue;
        value           uniform (0 0 -0.01);
    }

    Inlet2
    {
        type            fixedValue;
        value           uniform (0 0 -0.01);
    }

    Outlet
    {
        type            zeroGradient;
    }
    front
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    back
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    defaultFaces
    {
        type            empty;
    }
}

// ************************************************************************* //
